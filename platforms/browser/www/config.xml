<?xml version='1.0' encoding='utf-8'?>
<widget id="net.giansar.screenPatterns" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>giDemo Screen Patterns</name>
    <description>
        Screen Patterns for mobile with Angular Material
    </description>
    <author email="gpratiknya@gmail.com" href="http://giansar.net">
        Giansar Pratiknya
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
